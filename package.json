  "dependencies": {
    "chart.js": "^3.0.0-beta.4",
    "normalize.css": "^8.0.1",
    "potprox": "^0.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "clean-css-cli": "^4.3.0",
    "copyfiles": "^2.4.0",
    "eslint": "^7.11.0",
    "less": "^3.12.2",
