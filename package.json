{
  "name": "potprox-site",
  "version": "1.0.1",
  "description": "GitHub web page for the potprox project",
  "main": "index.js",
  "scripts": {
    "copyDependencies": "copyfiles -f ./node_modules/chart.js/dist/Chart.min.js ./node_modules/potprox/dist/potprox.min.js js/vendor",
    "lint": "eslint js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "terser js/main.js -c -m -o js/main.min.js && lessc css/main.less | cleancss --skip-rebase -O2 -o css/main.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amphiluke/potprox.git"
  },
  "keywords": [
    "potprox"
  ],
  "author": "Amphiluke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Amphiluke/potprox/issues"
  },
  "homepage": "https://amphiluke.github.io/potprox/",
  "dependencies": {
    "chart.js": "^2.9.3",
    "normalize.css": "^8.0.1",
    "potprox": "^0.7.2"
  },
  "devDependencies": {
    "clean-css-cli": "^4.3.0",
    "copyfiles": "^2.4.0",
    "eslint": "^7.11.0",
    "less": "^3.12.2",
    "terser": "^5.3.5"
  }
}
